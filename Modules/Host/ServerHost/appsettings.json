{
    "Logging": {
        "Console": {
            "FormatterName": "simple",
            "FormatterOptions": {
                "TimestampFormat": "dd/MM HH:mm:ss ",
                "IncludeScopes": false,
                "SingleLine": true
            }
        },
        "LogLevel": {
            "Default": "Warning",
            "Microsoft": "Warning",
            "System": "Warning",
            "ServerHost": "Debug",
            "ChatGPTAssistant": "Debug",
            "GoogleTextToSpeech": "Debug",
            "OpenAITextToSpeech": "Debug",
            "Microsoft.AspNetCore": "Warning"
        }
    },
  "AllowedHosts": "*"
}
